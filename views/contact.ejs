<%- include("partials/headerp"); -%>




 <!--            CONTACT US           -->

 
 <div  id="contactus">

  <div class="text-center">
    <img src="images/contact.png" class="responsive" width="300" height="200">
  </div>
  
    <div align="center">
      <div class="card">

        <!-- Card body -->
        <div class="card-body">   
      <form action="/" method="POST">
        
        <label for="email"></label>
        <input type="email" id="email" name="email" placeholder="Enter email ID"><br>
      
        <label for="fname"></label>
        <input type="text" id="fname" name="firstname" placeholder="Enter name"><br>
    
     <label for="category"></label>
        <select id="from" name="from">
          <option value="Select">Select</option>
          <option value="Student">Student</option>
          <option value="Staff">Staff</option>
          <option value="Alumni">Alumni</option>
          <option value="Institution">Institution</option>
           <option value="Company">Company</option>
        </select> <br>
    
        
        <textarea id="msg" name="msg" placeholder="Your message" style="height:200px"></textarea>
      <br>
          <input type="submit" value="Send">
          
      </form>
    </div>
    
    
    </div>
  </div>
    
    
</div>

